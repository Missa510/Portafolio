---
import Layout from "$_lay/layout.astro";
import ProjectsWrapper from "$_app/components/wrapper/ProjectWrapper";
import { slide } from "astro:transitions";
---

<Layout
    title="Missa510 ~ Proyectos"
    description="Estos son los proyectos de Missa510"
>
    <main class="container mx-auto mt-24">
        <ProjectsWrapper
            client:load
            transition:name="proyectos"
            transition:animate={slide({
                duration: "700ms",
            })}
        />
    </main>
</Layout>
